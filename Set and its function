a={1,2,3,4,5,6,7}
print(a)
a.remove(3)
print("remove=", a)
a. pop()
print("pop=", a)
a.discard(6)
print ("discard=",a)
a.update([8,9,10])
print("update=",a)
a. add(11)
print("add=",a)

a={1,2,3,4,5,6}
b={4,5,6,7,8,9}
print(a|b)
print (a&b)
print (a-b)
print (a^b)

#output
{1, 2, 3, 4, 5, 6, 7}
remove= {1, 2, 4, 5, 6, 7}
pop= {2, 4, 5, 6, 7}
discard= {2, 4, 5, 7}
update= {2, 4, 5, 7, 8, 9, 10}
add= {2, 4, 5, 7, 8, 9, 10, 11}
{1, 2, 3, 4, 5, 6, 7, 8, 9}
{4, 5, 6}
{1, 2, 3}
{1, 2, 3, 7, 8, 9}
